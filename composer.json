{
    "name": "bryceandy/press",
    "description": "A markdown blog for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "bryceandy",
            "email": "hello@bryceandy.com"
        }
    ],
    "require": {
        "php": "^7.4.*",
        "erusev/parsedown": "^1.7"
    },
    "require-dev": {
        "orchestra/testbench": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Bryceandy\\Press\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bryceandy\\Press\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bryceandy\\Press\\PressBaseServiceProvider"
            ]
        }
    }
}
